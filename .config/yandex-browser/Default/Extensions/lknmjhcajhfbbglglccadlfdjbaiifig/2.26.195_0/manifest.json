{
   "action": {
      "default_icon": "icon-16.png",
      "default_title": "Automatically record & transcribe Google Meet for FREE. 500k+ users"
   },
   "background": {
      "service_worker": "background.js"
   },
   "commands": {
      "pin-highlight": {
         "description": "Create a highlight during a meeting",
         "suggested_key": {
            "default": "Ctrl+Period",
            "mac": "Command+Period"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "content-scripts/google-calendar.js" ],
      "matches": [ "*://calendar.google.com/*" ],
      "run_at": "document_end"
   }, {
      "js": [ "content-scripts/google-meet.js" ],
      "matches": [ "*://meet.google.com/*" ],
      "run_at": "document_end"
   }, {
      "exclude_matches": [ "*://*.tldv.io/app/embed/*", "*://*.tldv.tech/app/embed/*", "*://*.zoom.us/*", "*://calendar.google.com/*", "*://meet.google.com/*" ],
      "js": [ "content-scripts/multi-tabs.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "description": "Automatically record & transcribe Google Meet for FREE. 500k+ users",
   "externally_connectable": {
      "matches": [ "*://tldv.io/*", "*://tldv.tech/*" ]
   },
   "host_permissions": [ "\u003Call_urls>" ],
   "icons": {
      "128": "icon-128.png",
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Record, Transcribe & ChatGPT for Google Meet",
   "optional_permissions": [ "clipboardWrite" ],
   "permissions": [ "tabs", "storage", "alarms" ],
   "short_name": "tl;dv",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.26.195"
}
